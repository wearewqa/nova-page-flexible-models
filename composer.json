{
  "name": "wqa/nova-page-flexible-models",
  "description": "A Laravel Nova package to allow relating models as flexible content to pages",
  "license": "MIT",
  "authors": [
    {
      "name": "Ben Sampson",
      "email": "ben.sampson@wearewqa.com"
    }
  ],
  "require": {
    "laravel/nova": "*",
    "whitecube/nova-flexible-content": "^0.2.7",
    "whitecube/nova-page": "^0.2.5|^1.0"
  },
  "autoload": {
    "psr-4": {
      "WQA\\NovaPageFlexibleModels\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "WQA\\NovaPageFlexibleModels\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
